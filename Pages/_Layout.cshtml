<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AsyncApi</title>

    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />

    <!-- Jquery -->
    <script src="~/lib/jquery/dist/jquery.js"></script>

    @*<meta http-equiv="Content-Security-Policy" content="upgrade-insecure-requests">*@

  
    <!-- Angular Modules -->

    <script src="~/js/angular.js"></script>
    <script src="~/js/angular-route.js"></script>

    <!-- Bootstrap JavaScript -->
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>

    <link href="~/lib/toastr/toastr.css" rel="stylesheet" />
    <script src="~/lib/toastr/toastr.js"></script>



    <!-- Application Modules -->
    <script src="~/app/app.js"></script>
    @*<script src="~/app/appRouteConfig.js"></script>*@
    <script src="~/app/appController.js"></script>
    <script src="~/app/sidebar/sidebarController.js"></script>

    <script src="~/app/home/homeController.js"></script>

    <script src="~/app/temp/tempController.js"></script>
    <script src="~/app/temp/tempDirective.js"></script>

    <!-- Gig -->
    <script src="~/app/gig/gigController.js"></script>
    <script src="~/app/gig/gigDirective.js"></script>

    <script src="~/app/chat/chatController.js"></script>
    
    <!-- Services -->
    <script src="~/app/services/notificationService.js"></script>


</head>
<body class="row">

    <div class="main-container body-content">

        <header>
            <div id="wrapper">
                <div class="title" style="margin-top: -15px;margin-bottom: -15px">Events Home</div>
                <div style="clear: both"></div>
            </div>
        </header>

        @RenderBody()

        <footer>
            <div><span>&copy; 2018</span><span text-align="right" style="float:right"></span></div>
        </footer>

    </div>



    @RenderSection("Scripts", required: false)

    <script>
        $(function () {
            console.log("ready!");
            angular.bootstrap(document, ["app"]);
        });
    </script>



</body>
</html>
